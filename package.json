{
  "name": "gq",
  "version": "0.0.1",
  "description": "A feature flag thingy",
  "main": "server.ts",
  "dependencies": {
    "@types/express": "^4.17.0",
    "command-line-args": "^5.1.1",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "io-ts": "^1.10.4",
    "io-ts-reporters": "0.0.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cloc": "^2.5.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.3.0",
    "ts-sinon": "^1.0.17",
    "tslint": "^5.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": "src",
    "reporter": [
      "lcov",
      "text"
    ],
    "temp-dir": "./.nyc_output/",
    "all": true
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .nyc_output coverage",
    "count": "cloc --by-file-by-lang src",
    "coverage": "nyc npm test",
    "lint-fix": "npm run lint -- --fix",
    "lint": "tslint --project .",
    "serve": "node --require ts-node/register --require source-map-support/register ./server.ts",
    "start": "node --require ts-node/register --require source-map-support/register ./index.ts",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abondoa/gq.git"
  },
  "keywords": [
    "feature-flags",
    "feature-toggle"
  ],
  "author": "Alex Bondo Andersen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abondoa/gq/issues"
  },
  "homepage": "https://github.com/abondoa/gq#readme"
}
